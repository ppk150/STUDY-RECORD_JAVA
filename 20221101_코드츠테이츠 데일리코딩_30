package com.codestates.coplit; 

public class Solution { 
	public int rotatedArraySearch(int[] rotated, int target) {
    // TODO:

      int count = 0 ;


    for(int i = 0 ; i < rotated.length ; i ++){
                if(rotated[i] ==target ){

                    return i ;

                }
            }

            if(count == 0){

                return -1 ;
            }
            return -1 ;



	} 
}
