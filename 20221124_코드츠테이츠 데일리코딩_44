public class codestatedaily_44 {
    public static void main(String[] args) {
        int[] output = new int[]{1, 2, 3};

        int max = -100000 ;

        int comp = 0;


        for(int i = 0 ; i < output.length ; i ++){

            comp = comp + output[i];

            if(comp > max){
                max = comp;
            }

            if(comp < 0 ){
                comp =0;

            }


        }

        System.out.println(max);

    }
}
