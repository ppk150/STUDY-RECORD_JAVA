package com.codestates.coplit; 

public class Solution { 
	public String makeMarginalString(String str) {
    // TODO:
    int L = str.length();
    String result = "";

    for(int i = 0 ; i < L ; i++){
      for(int j = 0 ; j <= i ; j++){

          result = result + String.valueOf(str.charAt(j));
    }
  }
  return result;
	
  } 
}
