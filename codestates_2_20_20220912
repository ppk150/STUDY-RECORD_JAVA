package com.codestates.coplit; 

public class Solution { 
	public boolean hasRepeatedCharacter(String str) {
    // TODO:
    int L = str.length();
    int count = 0;
    for(int i = 0 ; i < L ; i ++){
      for(int j = i+1 ; j < L ; j ++){
        if(str.charAt(i) == str.charAt(j)){
          count++;
        }
             
    }

    }
    if (count !=0){
      return true;
    }else{
      return false;
    }
	} 
}
